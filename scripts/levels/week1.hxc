import funkin.ui.story.Level;
import funkin.save.Save;
import funkin.data.story.level.LevelRegistry;

class Week1Level extends Level
{
  function new()
  {
    super('week1');
  }

  override function isUnlocked():Bool
  {
    super.isUnlocked();

    difficultyList = LevelRegistry.instance.fetchEntry('tutorial').getDifficulties();

    for (difficulty in difficultyList)
    {
      if (Save.instance.getLevelScore('tutorial', difficulty) == null)
      {
        continue;
      }
      else
      {
        return true;
      }
    }

    return false;
  }
}